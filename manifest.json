{
    "manifest_version": 3,
    "name": "Moonly - JavaScript Injector",
    "version": "1.0",
    "description": "Inject JavaScript into pages dynamically.",
    "permissions": [
        "tabs",
        "scripting",
        "storage",
        "activeTab",
        "webNavigation"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "libs/*"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup/popup.html",
        "default_icon": {
            "16": "icons/moon16.png",
            "32": "icons/moon32.png",
            "64": "icons/moon64.png",
            "128": "icons/moon128.png",
            "256": "icons/moon256.png",
            "512": "icons/moon512.png"
        }
    },
    "icons": {
        "16": "icons/moon16.png",
        "32": "icons/moon32.png",
        "64": "icons/moon64.png",
        "128": "icons/moon128.png",
        "256": "icons/moon256.png",
        "512": "icons/moon512.png"
    },
    "options_page": "script_manager.html"
}